{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "Array_of_UserClaimableRewardsResponse",
  "type": "array",
  "items": {
    "$ref": "#/definitions/UserClaimableRewardsResponse"
  },
  "definitions": {
    "UserClaimableRewardDetailResponse": {
      "type": "object",
      "required": [
        "claimable_reward",
        "level_index"
      ],
      "properties": {
        "claimable_reward": {
          "type": "integer",
          "format": "uint128",
          "minimum": 0.0
        },
        "level_index": {
          "type": "integer",
          "format": "uint8",
          "minimum": 0.0
        }
      },
      "additionalProperties": false
    },
    "UserClaimableRewardsResponse": {
      "type": "object",
      "required": [
        "claimable_reward",
        "claimable_reward_details",
        "reward_token"
      ],
      "properties": {
        "claimable_reward": {
          "type": "integer",
          "format": "uint128",
          "minimum": 0.0
        },
        "claimable_reward_details": {
          "type": "array",
          "items": {
            "$ref": "#/definitions/UserClaimableRewardDetailResponse"
          }
        },
        "reward_token": {
          "type": "string"
        }
      },
      "additionalProperties": false
    }
  }
}
